  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true  
      },
      "android": {
        "buildType": "apk",
        "env": {
          "ANDROID_KOTLIN_VERSION": "1.9.0",
          "EXPO_ANDROID_KOTLIN_VERSION": "1.9.0",
